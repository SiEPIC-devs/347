### Take a series command with different block
{sensor_control_true}, sensor_..., {stage_control_true}, stage_..., {tec_control_true}, tec_...

### Stage Control ###
stage_{x/y/z/chip/fiber}_step_{num}              # set move step
stage_{x/y/z/chip/fiber}_left_true               # move {x/y/z/chip/fiber} left 1 time
stage_{x/y/z/chip/fiber}_left_distance_{num}     # move {x/y/z/chip/fiber} {num} um
stage_{x/y/z/chip/fiber}_right_true              # move {x/y/z/chip/fiber} right 1 time
stage_{x/y/z/chip/fiber}_right_distance_{num}    # move {x/y/z/chip/fiber} {num} um

stage_stop_true                                  # stop movement
stage_zero_true                                  # set zero
stage_laod_true                                  # load the devices
stage_move_true                                  # move to devices
stage_lock_true                                  # lock stage control
stage_unlock_true                                # unlock stage control

stage_lim_x_left_{num}                           # set x left limit
stage_lim_x_right_{num}                          # set x right limit
stage_lim_y_left_{num}                           # set y left limit
stage_lim_y_right_{num}                          # set y right limit
stage_lim_z_left_{num}                           # set z left limit
stage_lim_z_right_{num}                          # set z right limit
stage_lim_confirm_true                           # confirm the change of limit setting

stage_fa_x_count_{num}                           # setting the fine align x count
stage_fa_x_step_{num}                            # setting the fine align x step
stage_fa_y_count_{num}                           # setting the fine align y count
stage_fa_y_step_{num}                            # setting the fine align y step

stage_as_x_count_{num}                           # setting the area scan x count
stage_as_x_step_{num}                            # setting the area scan x step
stage_as_y_count_{num}                           # setting the area scan y count
stage_as_y_step_{num}                            # setting the area scan y step

sensor_control_true,sensor_on_true, stage_control_true,stage_x_step_11,stage_y_step_12,tec_control_true,tec_on_true
stage_control_true,tec_control_true
sensor_control_true,sensor_on_true, tec_control_true,tec_on_true,stage_control_true,stage_x_step_11,stage_y_step_12

### TEC Control ###
tec_on_true                                      # open the TEC
tec_off_true                                     # close the TEC
tec_tem_{num}                                    # set the temperature

### Sensor Control ###
sensor_on_true                                   # open the laser
sensor_off_true                                  # close the laser
sensor_wvl_{num}                                 # set wavelength
sensor_pwr_{num}                                 # set power
sensor_wvl_{num}                                 # set wavelength
sensor_range_start_{num}                         # set range start
sensor_range_end_{num}                           # set range end

